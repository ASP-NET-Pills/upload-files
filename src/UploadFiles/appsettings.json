{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 1073741824,
      "MaxConcurrentConnections": null,
      "MinRequestBodyDataRate": null,
      "MinResponseDataRate": null
    }
  },
  "FormRequests": {
    "MultipartBodyLengthLimit": 1073741824
  }
}
